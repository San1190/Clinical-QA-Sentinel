{
    "browser": "chrome",
    "env": "demo",
    "timeout": 10,
    "headless": false,
    "environments": {
        "dev": {
            "base_url": "http://localhost:8080",
            "username": "admin",
            "password": "password"
        },
        "demo": {
            "base_url": "https://katalon-demo-cura.herokuapp.com",
